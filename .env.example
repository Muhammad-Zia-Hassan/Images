# GitHub Configuration for Images Manager
# Copy this file to .env and fill in your values

# GitHub Personal Access Token with 'repo' scope
# Create one at: https://github.com/settings/tokens
VITE_GITHUB_TOKEN=token

# GitHub repository in format: username/repository-name
# Example: johndoe/my-images
VITE_GITHUB_REPO=Muhammad-Zia-Hassan/github-images-manager

# Branch to store images (default: main)
VITE_GITHUB_BRANCH=main
